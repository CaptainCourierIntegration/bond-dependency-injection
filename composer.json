{
    "name": "bond/dependency-injection",
    "type": "library",
    "description": "Symfony's Dependency Injection Extensions",
    "keywords": ["Di", "Symfony", "DependencyInjection", "Dependency Injection"],
    "homepage": "http://captaincourier.org",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Joseph",
            "email": "joseph@captaincourier.org",
            "role": "Developer"
        },
        {
            "name": "Peter",
            "email": "peter@captaincourier.org",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "files": [
            "src/Bond/functions.php"
        ]
    },
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/yaml": "*",
        "symfony/config": "*"
    },
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 5000
    }
}
